{
  "name": "drd-plus/frontend-skeleton",
  "license": "MIT",
  "type": "library",
  "description": "Skeleton for DrD+ pages",
  "keywords": [
    "drd plus",
    "pages",
    "view",
    "frontend",
    "helper",
    "skeleton"
  ],
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/",
      "Tests\\": "Tests/"
    }
  },
  "bin": [
    "bin/assets"
  ],
  "require": {
    "php": ">=7.1",
    "ext-mbstring": "*",
    "phpgt/dom": "~1.0",
    "piwik/device-detector": "^3.9",
    "tracy/tracy": "^2.4",
    "phpmailer/phpmailer": "^6.0",
    "granam/string": "dev-master",
    "granam/exceptions-hierarchy": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "roave/security-advisories": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
